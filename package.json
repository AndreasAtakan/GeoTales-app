{
	"name": "geotales",
	"version": "1.0.0",
  "description": "GeoTales app",
	"packageManager": "yarn@3.2.0",
	"scripts": {
		"start:wsl:wifi": "REACT_NATIVE_PACKAGER_HOSTNAME=$(netsh.exe interface ip show address \"Wi-Fi\" | grep 'IP Address' | sed -r 's/.*IP Address:\\W*//') TAMAGUI_TARGET=native expo start",
		"start:wsl:eth": "REACT_NATIVE_PACKAGER_HOSTNAME=$(netsh.exe interface ip show address \"Ethernet\" | grep 'IP Address' | sed -r 's/.*IP Address:\\W*//') TAMAGUI_TARGET=native expo start",
		"start": "expo start",
		"android": "expo run:android",
		"ios": "expo run:ios",
		"web": "expo start --web",
		"eject": "expo eject",
		"upgrade:tamagui": "manypkg upgrade tamagui && manypkg upgrade @tamagui && manypkg upgrade tamagui-loader"
	},
	"dependencies": {
		"@react-navigation/native": "6.1.1",
		"@react-navigation/native-stack": "6.9.7",
		"@tamagui/animations-reanimated": "1.0.21",
		"@tamagui/font-inter": "1.0.21",
		"@tamagui/lucide-icons": "1.0.21",
		"@tamagui/shorthands": "1.0.21",
		"@tamagui/theme-base": "1.0.21",
		"expo": "47.0.13",
		"expo-font": "11.0.1",
		"expo-linear-gradient": "12.0.1",
		"expo-updates": "~0.15.6",
    "expo-asset": "~8.7.0",
    "expo-constants": "~14.0.2",
    "expo-document-picker": "^11.0.1",
    "expo-image-picker": "^14.0.2",
    "expo-linking": "~3.3.0",
    "expo-splash-screen": "~0.17.5",
    "expo-status-bar": "~1.4.2",
    "expo-system-ui": "~2.0.1",
    "expo-web-browser": "~12.0.0",
		"react": "18.1.0",
		"react-dom": "18.1.0",
		"react-native": "0.70.5",
		"react-native-gesture-handler": "2.8.0",
		"react-native-reanimated": "2.12.0",
		"react-native-safe-area-context": "4.4.1",
		"react-native-screens": "3.18.2",
		"react-native-svg": "13.4.0",
		"react-native-web": "0.18.10",
		"tamagui": "1.0.21"
	},
	"resolutions": {
		"react-native-svg": "13.4.0"
	},
	"devDependencies": {
		"@babel/core": "7.20.2",
		"@manypkg/cli": "0.19.2",
		"@tamagui/babel-plugin": "1.0.21",
		"@types/react": "18.0.25",
		"@types/react-native": "0.70.6",
		"babel-plugin-transform-inline-environment-variables": "0.4.4",
		"eslint": "8.28.0",
		"eslint-config-universe": "11.1.1",
		"prettier": "2.7.1",
		"typescript": "4.9.3"
	},
  "repository": {
    "type": "git",
    "url": "git+https://github.com/AndreasAtakan/GeoTales-app.git"
  },
  "author": "NFEDB",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/AndreasAtakan/GeoTales-app/issues"
  },
  "homepage": "https://github.com/AndreasAtakan/GeoTales-app#readme"
}
